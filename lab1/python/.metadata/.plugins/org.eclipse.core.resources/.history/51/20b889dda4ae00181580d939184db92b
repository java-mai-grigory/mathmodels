import math
import scipy.integrate as integrate


def f(x):
    k = math.sin(math.pi / 4)
    return 1/ math.sqrt(1 - k**2 * math.sin(x)**2 )

     
def eliptic():
    return integrate.quad(f, 0, math.pi / 2)[0]

def seqtest():
    P = S = 1.0
    k = math.sin(math.pi / 8)
    N = 1000
    for i in range(1, N):
        P *= (2* i - 1) / (2 * i);
        S += P * P * k * k
    return S * math.pi / 2    


print( eliptic() )
print(seqtest())